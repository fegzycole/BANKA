import testData from '../data/testData';
const { accounts } = testData;

static CreateNewAccount() {
    const number = 200000000 + (Math.floor(Math.random() * 5000));
    // eslint-disable-next-line max-len
    const existingAccountNo = accounts.reduce((acctNoArray, userDetail) => acctNoArray.concat(userDetail.accountNo), []);
    if (existingAccountNo.includes(number)) {
      this.CreateNewAccount();
    }
    return number;
  }