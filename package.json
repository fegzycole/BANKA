{
  "name": "banka-project",
  "version": "1.0.0",
  "description": "Banking Application",
  "main": "app.js",
  "scripts": {
    "test": "mocha Server/tests/*.js --require @babel/register --timeout 50000 --exit || true",
    "test-watch": "nodemon --exec \"npm test\"",
    "start": "node dist/app.js",
    "start-dev": "nodemon --exec babel-node server/app.js",
    "babel-build": "babel Server -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov| coveralls",
    "coveralls": "nyc --reporter-lcov --reporter=text-lcov npm test",
    "build": "npm run clean && npm run babel-build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fegzycole/Banka-Project.git"
  },
  "author": "Iyara Ferguson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fegzycole/Banka-Project/issues"
  },
  "homepage": "https://github.com/fegzycole/Banka-Project#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@sendgrid/mail": "^6.3.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "nyc": "^13.3.0",
    "pg": "^7.10.0",
    "swagger-ui-express": "^4.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.1.2",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "directories": {
    "test": "tests"
  }
}
