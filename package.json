{
  "name": "banka-project",
  "version": "1.0.0",
  "description": "Banking Application",
  "main": "app.js",
  "scripts": {
    "test": "mocha Server/tests/*.js --require @babel/register --timeout 50000 --exit || true",
    "test-watch": "nodemon --exec \"npm test\"",
    "start": "node dist/app.js",
    "start-dev": "nodemon --exec babel-node server/app.js",
    "babel-build": "babel Server -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov| coveralls",
    "coveralls": "nyc --reporter-lcov --reporter=text-lcov npm test",
    "build": "npm run clean && npm run babel-build",
    "heroku-postbuild": "npm run build",
    "migration": "npx sequelize-cli db:migrate && npm run seeder",
    "undo-migration": "npx sequelize-cli db:migrate:undo:all",
    "seeder": "npx sequelize db:seed:all",
    "undo-seeder": "npx sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fegzycole/Banka-Project.git"
  },
  "author": "Iyara Ferguson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fegzycole/Banka-Project/issues"
  },
  "homepage": "https://github.com/fegzycole/Banka-Project#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@sendgrid/mail": "^6.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-body-trimmer": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "nyc": "^13.3.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "pg": "^7.14.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "swagger-ui-express": "^4.1.2",
    "validatorjs": "^3.18.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.19.1",
    "gh-pages": "^2.1.1",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
